(function(w, d) {
	var src_co_io = 'auto';
	var tld = '';

	if(src_co_io === 'co' || src_co_io === 'io') {
		tld = src_co_io;
		sessionStorage.setItem('smc_cdn_source', tld);
	} else {
		tld  = sessionStorage.getItem('smc_cdn_source');
	}

	var cspCheck = function(cb) {
		if(tld !== null) {
			cb();
		} else {
			tld = 'io';
			var ioTest = new XMLHttpRequest();
			ioTest.open('HEAD', 'https://js.smct.' + tld + '/csp/csp.js', true);
			ioTest.send();
			ioTest.onerror = function() {
				tld = 'co'
				var coTest = new XMLHttpRequest();
				coTest.open('HEAD', 'https://js.smct.' + tld + '/csp/csp.js', true);
				coTest.send();
				coTest.onerror = function() {
					tld = 'co';
					cb();
				}
				coTest.onload = function() {
					tld = 'co';
					sessionStorage.setItem('smc_cdn_source', tld);
					cb();
				}
			}
			ioTest.onload = function() {
				tld = 'io';
				sessionStorage.setItem('smc_cdn_source', tld);
				cb();
			}
		}
	}

	var addJS = function(scriptUrl, callback) {
        var url = window.$smctResources.getURL(scriptUrl);
        var sriValue = window.$smctResources.getSRI(scriptUrl);
			if(url.indexOf('://l.smct') === -1 && url.indexOf('://dev.smct') === -1 && tld !== undefined) {
				url = url.replace(/smct.(c|i)o/i, 'smct.' + tld);
			}
		var h = d.getElementsByTagName("head")[0],
			j = d.createElement('script');
		j.type = 'text/javascript';
		j.src = url;
		j.charset = 'utf-8';
		h.appendChild(j);
		j.onload = function() {
			if(callback) {
				callback();
			}
		};
	}

	try{
		cspCheck(function cspChecker() {
			addJS('tag_url', function addingJS() {
				window.$smcCallCustomScripts = function() {
					$smcT5.CustomScripts = {};
					try {
						$smcT5.CustomScripts.TagCode = function(){
    (function BasketScanner(){
        var getATC = setInterval(function(){
            var atc = document.querySelector('#productPageAdd2Cart');
            if(atc){
                atc.addEventListener('click', function(){
                    var sku = window.location.href.split('-').pop();
                    var title = document.querySelector('#estore_product_title h1');
                    var price = document.querySelector('#PDP_productPrice');
                    if (title && price){
                        var obj = {};
                        obj.img = `https://boots.scene7.com/is/image/Boots/${sku}?wid=114&hei=114&op_sharpen=1`;
                        obj.title = title.textContent.trim();
                        obj.price = price.textContent.trim();
                        var itemEncoded = btoa(encodeURIComponent(JSON.stringify(obj)));
                        $smcT5.CookieManager.create('smc_dyn_item', itemEncoded, 0);
                    }
                });
                clearInterval(getATC);
            }
        }, 400);
    })();
    
    (function BasketScanner(){
        var getATC = setInterval(function(){        
            var img = document.querySelector('div[class$="basket__content"] div[class$="product-details"] img,div[class$="order-summary__body"] div[class$="product-details"] img');
            var title = document.querySelector('div[class$="basket__content"] div[class$="product-details"] p[class$="product-details__name"],div[class$="order-summary__body"] div[class$="product-details"] p[class$="product-details__name"]');
            var price = document.querySelector('div[class$="basket__content"] div[class$="product-details"] p[class$="product-details__price"],div[class$="order-summary__body"] div[class$="product-details"] p[class$="product-details__price"]');
            if (img && title && price){
                var obj = {};
                obj.img = img.src;
                obj.title = title.textContent.trim();
                obj.price = price.textContent.trim();
                var itemEncoded = btoa(encodeURIComponent(JSON.stringify(obj)));
                $smcT5.CookieManager.create('smc_dyn_item', itemEncoded, 0);
            }
        }, 400);
    })();
    
    (function cartButtonListener(){
        setInterval($smcT5.CustomScripts.multiProductScan, 2000);
    })();
};

$smcT5.CustomScripts.DynamicItemExist = function(){
    var cookie = $smcT5.CookieManager.read('smc_dyn_item');
    return cookie ? true : false;
};

$smcT5.CustomScripts.Currency = function(){
    var href = window.location.href;
    if(href.indexOf('/process-order') > -1 && data && data.transaction && data.transaction.currency){
        return data.transaction.currency;
    }
    return null;
};

$smcT5.CustomScripts.multiProductScan = function(){
    var mainContainerCart = document.querySelector('.product_listing_container');
    var href = window.location.href;
    if(mainContainerCart && href.indexOf('/tuesday-offer') > -1){
        var itemContainer = mainContainerCart.querySelectorAll('.estore_product_container');
        var productDetails = [];
        for(var i = 0; i < itemContainer.length; i++){
            var product = itemContainer[i];
            if(i <= 15){
                var imageEl = product.querySelector('.product_img_link img');
                var titleEl = product.querySelector('.product_name a');
                var priceNowEl = product.querySelector('.product_info .product_price');
                var priceWasEl = product.querySelector('.product_info .product_savePrice .was');
                var skuEl = product.querySelector('input[id^="dlProductId"]');
                var linkEl = product.querySelector('.product_img_link');
                if(imageEl && titleEl && priceNowEl && priceWasEl && skuEl && linkEl){
                    var image = imageEl.src;
                    var title = titleEl.textContent;
                    var price = priceNowEl.textContent;
                    var priceWas = priceWasEl.textContent;
                    var sku = skuEl.value.replace('.P','');
                    var link = linkEl.href;
                    productDetails.push({ image: image, name: title, price: price, priceWas: priceWas, sku: sku, link: link });
                }
            }
        }
        var itemEncoded = btoa(encodeURIComponent(JSON.stringify(productDetails)));
        localStorage.setItem('smc_all_product', itemEncoded, 0);
    }        
};

$smcT5.CustomScripts.TuesdayProductExist = function(){
    const weekday = ["Sunday","Monday","Tuesday","Wednesday","Thursday","Friday","Saturday"];
    const d = new Date();
    let day = weekday[d.getDay()];
    var cookie = localStorage.getItem('smc_all_product');
    return cookie && day === 'Tuesday' ? true : false;
};

$smcT5.CustomScripts.PopupActive = function(){
    const el = document.querySelector('.usabilla__overlay');
    const el1 = document.querySelector('.usabilla_scroller_area');
    if(el && el.style.display === 'block') {
        return true;
    } else if(el1) {
        return true;
    }
    return false;
};
					} catch(e) {
						console.error("CustomScripts Error: ", e);
					}
				}
			});
		});
	} catch(e) {
		console.log(e);
	}
}(window, document));

